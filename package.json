{
    "name": "awel",
    "version": "0.3.0",
    "private": false,
    "type": "module",
    "bin": {
        "awel": "./bin/awel.js"
    },
    "files": [
        "dist/",
        "bin/",
        "babel-plugin-awel-source.cjs"
    ],
    "scripts": {
        "build:host": "cd packages/host && esbuild src/index.ts --bundle --minify --outfile=../../dist/host/host.js --format=iife",
        "build:dashboard": "cd packages/dashboard && vite build",
        "build:cli": "tsc -p packages/cli && cp -r packages/cli/src/skills dist/cli/skills",
        "build": "npm run build:host && npm run build:dashboard && npm run build:cli",
        "dev": "tsc -p packages/cli --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^3.0.31",
        "@ai-sdk/google": "^3.0.18",
        "@ai-sdk/openai": "^3.0.21",
        "@clack/prompts": "^0.9.1",
        "@hono/node-server": "^1.13.7",
        "@openrouter/ai-sdk-provider": "^2.1.1",
        "@types/http-proxy": "^1.17.17",
        "ai": "^6.0.57",
        "ai-sdk-provider-claude-code": "^3.3.3",
        "ai-sdk-provider-gemini-cli": "^2.0.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "commander": "^12.1.0",
        "diff": "^7.0.0",
        "execa": "^9.5.2",
        "fast-glob": "^3.3.3",
        "hono": "^4.6.14",
        "http-proxy": "^1.18.1",
        "i18next": "^25.8.0",
        "lucide-react": "^0.469.0",
        "open": "^11.0.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-i18next": "^16.5.4",
        "react-markdown": "^10.1.0",
        "tailwind-merge": "^2.6.0",
        "turndown": "^7.2.2",
        "vercel-minimax-ai-provider": "^0.0.2",
        "ws": "^8.19.0",
        "zhipu-ai-provider": "^0.2.2",
        "zod": "^4.0.0"
    },
    "devDependencies": {
        "@types/diff": "^7.0.2",
        "@types/node": "^22.10.5",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@types/turndown": "^5.0.6",
        "@types/ws": "^8.18.1",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "^4.0.18",
        "autoprefixer": "^10.4.20",
        "esbuild": "^0.24.2",
        "jsdom": "^28.0.0",
        "postcss": "^8.4.49",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.3.3",
        "vite": "^6.0.7",
        "vitest": "^4.0.18"
    }
}
